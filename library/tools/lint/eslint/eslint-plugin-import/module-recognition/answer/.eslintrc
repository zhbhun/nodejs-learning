{
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "plugins": ["eslint-plugin-import"],
  "rules": {
    "import/no-unresolved": "error",
    "import/named": "error",
    "import/default": "error"
  },
  "settings": {
    "import/extensions": [".js", ".jsx"]
  }
}
